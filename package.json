    "gatsby": "^5.0.0",
    "gatsby-plugin-mdx": "^5.0.0",
    "gatsby-plugin-sass": "^6.0.0",
    "gatsby-source-filesystem": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.6.0",
    "react-lazy-load-image-component": "^1.5.6",
    "sass": "^1.56.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
