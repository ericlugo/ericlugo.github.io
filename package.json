{
  "name": "eric-lugo-dev-blog",
  "version": "1.0.0",
  "private": true,
  "description": "eric-lugo-dev-blog",
  "author": "Eric Lugo",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "add-domain": "echo \"ericlugo.dev\" > public/CNAME",
