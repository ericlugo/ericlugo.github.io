    "stop-telemetry": "gatsby telemetry --disable"
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.5.12",
    "@fontsource/ibm-plex-sans": "^4.5.13",
    "@mdx-js/react": "^2.1.5",
    "gatsby": "^5.0.0",
    "gatsby-plugin-mdx": "^5.0.0",
    "gatsby-plugin-sass": "^6.0.0",
    "gatsby-source-filesystem": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  }
}
